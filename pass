from(bucket:"mybucket")
  |> range(start:-1h)
  |> filter(fn:(r) =>
      r._field == "memory_usage_bytes" and
      r.namespace == "default" and
      r.container_name == "nginx"
  )


  from(bucket:"mybucket")
  |> range(start:-1h)
  |> filter(fn:(r) =>
      r._field == "cpu_usage_core_nanoseconds" and
      r.namespace == "default" and
      r.container_name == "nginx"
  )